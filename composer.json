{
    "name": "glue-systems/sp-api-open-api",
    "description": "OpenAPI-generated PHP clients for Amazon Selling Partner API",
    "type": "library",
    "keywords": [
        "glue",
        "amazon",
        "sp-api",
        "selling-partner",
        "php",
        "client"
    ],
    "license": "unlicense",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }, {
            "name": "Glue Systems Dev Team",
            "role": "Organize API clients into package-able library"
        }
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/support": "4.2.*",
        "illuminate/cache": "4.2.*",
        "nesbot/carbon": "1.38.3",
        "aws/aws-sdk-php": "^3.277"
    },
    "autoload": {
        "psr-4": {
            "Glue\\SPAPI\\OpenAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*",
        "vlucas/phpdotenv": "^3.3",
        "friendsofphp/php-cs-fixer": "~2.12"
    }
}
