{
    "name": "glue-systems/sp-api-open-api",
    "description": "OpenAPI-generated PHP clients for Amazon Selling Partner API",
    "type": "library",
    "keywords": [
        "glue",
        "amazon",
        "sp-api",
        "selling-partner",
        "php",
        "client"
    ],
    "license": "unlicense",
    "authors": [
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }, {
            "name": "Glue Systems Dev Team",
            "role": "Organize API clients into package-able library"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/contracts": "5.8.*",
        "illuminate/support": "5.8.*",
        "illuminate/cache": "5.8.*"
    },
    "autoload": {
        "psr-4": {
            "Glue\\SPAPI\\OpenAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12",
        "vlucas/phpdotenv": "^3.3"
    }
}
